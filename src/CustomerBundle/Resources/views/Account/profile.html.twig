{% extends "@Customer/Default/index.html.twig" %}
{% block body %}
    <div class="header" style="color-interpolation: linearRGB;background-color: #7DA0B1;color:white">
      <h1 align="center">jinikart</h1>       
    </div>
    <div class="customerform" style=" padding: 20px 15px 15px 20px; left: 100px ; margin-left:30em">

            {{ form_start(form) }}
            {{ form_row(form.fname) }}
            {{ form_row(form.lname) }}
            {{ form_row(form.email) }}
            {{ form_row(form.mobile_no) }}         
            {{ form_row(form.address_line1) }}
            {{ form_row(form.address_line2) }}
            {{ form_row(form.pincode) }}
            {{ form_row(form.state) }}
            {{ form_row(form.country) }}
            {{ form_row(form.question1) }}
            {{ form_row(form.question2) }}

            <br/>
            <div id="buttons" class="btn">
            <button type="submit">Register!</button>
            <button type="reset">Clear</button>
            </div>
            {{ form_end(form) }}
        </div>
    
{% endblock %}